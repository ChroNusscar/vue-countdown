{
  "name": "vue-countdown",
  "description": "Countdown component for Vue.js.",
  "version": "0.2.0",
  "main": "dist/vue-countdown.common.js",
  "module": "dist/vue-countdown.esm.js",
  "browser": "dist/vue-countdown.js",
  "repository": "xkeshi/vue-countdown",
  "license": "MIT",
  "author": "xkeshi",
  "keywords": [
    "vue",
    "vuejs",
    "vue.js",
    "vue-component",
    "countdown",
    "component",
    "html",
    "css",
    "javascript",
    "front-end",
    "web",
    "development"
  ],
  "scripts": {
    "dev": "rollup -c -m -w",
    "lint": "eslint ./src",
    "build": "rollup -c",
    "compress": "uglifyjs ./dist/vue-countdown.js -o ./dist/vue-countdown.min.js -c -m --comments /^!/",
    "prebuild": "npm run lint",
    "postbuild": "npm run compress"
  },
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-watch": "^3.2.2",
    "uglify-js": "^2.8.22"
  },
  "peerDependencies": {
    "vue": "^2.2.0"
  }
}
